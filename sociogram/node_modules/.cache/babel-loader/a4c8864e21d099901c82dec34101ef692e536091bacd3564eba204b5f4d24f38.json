{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  data() {\n    return {\n      userId: \"63db40eff9d8e72fa3015fd7\",\n      datePosted: \"\",\n      typeFlag: false,\n      fileType: localStorage.getItem(\"fileType\"),\n      fileURL: localStorage.getItem(\"fileURL\"),\n      caption: \"MAc Book Pro !!!\",\n      url: \"\",\n      fileName: \"\",\n      iv: undefined\n    };\n  },\n  created() {\n    // debugger;\n    this.iv = localStorage.getItem(\"fileType\");\n    console.log(\"type from postinfo :\" + this.iv);\n    if (this.iv === \"images\") {\n      this.typeFlag = true;\n      console.log(\"Uploaded Images\");\n    } else {\n      this.typeFlag = false;\n      console.log(\"Uploaded Video\");\n    }\n  },\n  methods: {\n    uploadHandle() {\n      localStorage.clear();\n      const requestbody = {\n        userId: this.userId,\n        datePosted: new Date(),\n        fileType: this.fileType,\n        filePostedURL: this.url,\n        caption: this.caption\n      };\n      axios.post(\"http://localhost:8080/user/addPost\", requestbody);\n    }\n  }\n};","map":{"version":3,"mappings":"AAeA;AACA;EACAA;IACA;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACA;IACA;IACAC;IACA;MACA;MACAA;IACA;MACA;MACAA;IACA;EACA;EACAC;IACAC;MACAC;MACA;QACAb;QACAC;QACAE;QACAW;QACAT;MACA;MACAU;IACA;EACA;AACA","names":["data","userId","datePosted","typeFlag","fileType","fileURL","caption","url","fileName","iv","created","console","methods","uploadHandle","localStorage","filePostedURL","axios"],"sourceRoot":"src/views","sources":["PostInfo.vue"],"sourcesContent":["<template>\n  <div class=\"post-info\">\n    <!-- {{ this.fileURL }} -->\n    <img v-if=\"typeFlag\" alt=\"Loading ....\" :src=\"this.fileURL\" />\n    <!-- // eslint-disable-next-line vue/no-parsing-error -->\n    <video v-else :src=\"this.fileURL\" frameborder=\"0\" controls />\n    <input\n      type=\"text\"\n      placeholder=\"Place your Caption Here\"\n      class=\"post-caption\"\n    />\n    <button class=\"upload-post\" @click=\"uploadHandle\">Done</button>\n  </div>\n</template>\n<script>\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      userId: \"63db40eff9d8e72fa3015fd7\",\n      datePosted: \"\",\n      typeFlag: false,\n      fileType: localStorage.getItem(\"fileType\"),\n      fileURL: localStorage.getItem(\"fileURL\"),\n      caption: \"MAc Book Pro !!!\",\n      url: \"\",\n      fileName: \"\",\n      iv: undefined,\n    };\n  },\n  created() {\n    // debugger;\n    this.iv = localStorage.getItem(\"fileType\");\n    console.log(\"type from postinfo :\" + this.iv);\n    if (this.iv === \"images\") {\n      this.typeFlag = true;\n      console.log(\"Uploaded Images\");\n    } else {\n      this.typeFlag = false;\n      console.log(\"Uploaded Video\");\n    }\n  },\n  methods: {\n    uploadHandle() {\n      localStorage.clear();\n      const requestbody = {\n        userId: this.userId,\n        datePosted: new Date(),\n        fileType: this.fileType,\n        filePostedURL: this.url,\n        caption: this.caption,\n      };\n      axios.post(\"http://localhost:8080/user/addPost\", requestbody);\n    },\n  },\n};\n</script>\n\n<style scoped>\n.post-image {\n  width: 30vw;\n  height: 50vh;\n}\n.post-caption {\n  width: 40vw;\n  height: 5vh;\n  background-color: whitesmoke;\n}\n.post-info {\n  display: flex;\n}\n\n.post-upload {\n  width: 10vw;\n  height: 4vh;\n}\n.post-video {\n  width: 30vw;\n  height: 40vh;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}