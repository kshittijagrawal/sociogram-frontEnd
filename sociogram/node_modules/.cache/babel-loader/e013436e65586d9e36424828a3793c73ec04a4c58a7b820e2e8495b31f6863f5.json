{"ast":null,"code":"export default {\n  name: \"PostList\",\n  emits: [\"parent-emit\"],\n  props: {\n    list: {\n      type: Array,\n      default: () => [],\n      required: true\n    }\n  },\n  methods: {\n    emitParent(post) {\n      localStorage.setItem(\"productID\", post.productID);\n      // this.$router.push(\"/productdescription\");\n      //   this.$emit(\"parent-emit\", product);\n    }\n  }\n};","map":{"version":3,"mappings":"AAmBA;EACAA;EACAC;EACAC;IACAC;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACAC;MACAC;MACA;MACA;IACA;EACA;AACA","names":["name","emits","props","list","type","default","required","methods","emitParent","localStorage"],"sourceRoot":"src/components","sources":["PostList.vue"],"sourcesContent":["<template>\n  <div id=\"product-list\">\n    <div\n      @click=\"emitParent(post)\"\n      v-for=\"(post, index) in postList\"\n      :key=\"index\"\n    >\n      <p><img :src=\"post.imageURL\" alt=\"image not found\" /></p>\n      <p class=\"product-name\">\n        <b\n          ><i>{{ post.productName }}</i></b\n        >\n      </p>\n      <slot></slot>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"PostList\",\n  emits: [\"parent-emit\"],\n  props: {\n    list: {\n      type: Array,\n      default: () => [],\n      required: true,\n    },\n  },\n  methods: {\n    emitParent(post) {\n      localStorage.setItem(\"productID\", post.productID);\n      // this.$router.push(\"/productdescription\");\n      //   this.$emit(\"parent-emit\", product);\n    },\n  },\n};\n</script>\n<style scoped></style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}